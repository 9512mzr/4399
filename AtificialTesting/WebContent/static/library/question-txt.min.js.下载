OT2.QuestionTxt=function(){var t=function(t,e,n){this.model=t,this.Evt=e,this.$el=null,this.option={className:".exam-s",template:"question-txt",childName:".exam-qlist"},void 0!==n&&_.extend(this.option,n)};return t.prototype={template:function(t){var e=OT2.Util.Template().get(this.option.template);return _.template(e,t)},render:function(e){var n=this;return n.$el=$(n.template(n.model)),n.txtBlank(),n.widthAuto(),"function"==typeof e&&e.call(n),"7"==n.model.question_type&&n.model.list.length>0&&_.each(n.model.list,function(e,i){e.index=i;var o=new t(e,n.Evt,n.option);o.render(),n.$el.find(n.option.childName).append(o.$el)}),n.$el.find("img").attr("draggable",!1).on("dragstart",function(){return!1}),this},widthAuto:function(){var t=this;if("1236".indexOf(t.model.question_type)<0)return!1;t.Evt.subscribe("question:width",function(){t.$el.find(t.option.className).each(function(){OT2.Util.calcItemWidth($(this))})})},txtBlank:function(){if("7"==this.model.question_type)return!1;this.$el.find(".cke_questions_blankInput").each(function(t){$(this).after('<span class="q-blank">&nbsp;</span>'),$(this).remove()}),this.$el.find("span.filling").each(function(t){$(this).after('<span class="q-blank q-blank2">'+(t+1)+"</span>"),$(this).remove()})}},t}();