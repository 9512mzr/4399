OT2.Cookie={set:function(e,t,n,o,i){void 0===n&&(n=new Date((new Date).getTime()+36e5)),n=n?"; expires="+n.toGMTString():"",o=o?"; path="+o:"; path=/",i=i?";domain="+i:"",document.cookie=e+"="+escape(t)+n+o+i},get:function(e){var t=document.cookie.match(new RegExp("(^| )"+e+"=([^;]*)(;|$)"));return null!=t?unescape(t[2]):null},clear:function(e,t,n){this.get(e)&&(t=t?"; path="+t:"; path=/",n=n?"; domain="+n:"",document.cookie=e+"="+t+n+";expires=Fri, 02-Jan-1970 00:00:00 GMT")}};